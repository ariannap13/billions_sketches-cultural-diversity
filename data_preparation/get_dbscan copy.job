#!/bin/bash

#SBATCH --job-name=dbscan      # Job name
#SBATCH --cpus-per-task=4       # Schedule one core
#SBATCH --time=20:00        # Run time (hh:mm:ss) - run for one hour max

# Set the output file name dynamically
OUTPUT_FILE="dbscan_$1.out"

# Redirect the script's output to the dynamic file
exec > "$OUTPUT_FILE" 2>&1

python dbscan_run.py $1 $2 $3 $4
